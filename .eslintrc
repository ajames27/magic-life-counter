{
  "parser": "babel-eslint",
  "env": {
    "browser": true
  },
  "plugins": ["react", "react-native"],
  "extends": ["eslint:recommended", "plugin:react/recommended"],
  "rules": {
    "react-native/no-unused-styles": 2,
    "react-native/no-inline-styles": 1,
    "react-native/no-color-literals": 0,
    "no-unused-vars": 1,
    "no-console": 0
  }
}
